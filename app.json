{
  "expo": {
    "name": "WorkezyApp",
    "slug": "WorkezyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.workezy.WorkezyApp",
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS"
      ],
      "usesCleartextTraffic": true,
      "queries": [
        {
          "package": "com.whatsapp"
        },
        {
          "intent": "android.intent.action.DIAL",
          "data": {
            "scheme": "tel"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.0",
            "gradlePluginVersion": "8.7.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "07b9d9e4-78ed-490b-8366-4e1b4dcd9837"
      }
    },
    "owner": "workezy"
  }
}